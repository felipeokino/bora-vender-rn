{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "module": "commonjs",
    "target": "ES2020",
    "jsx": "react",
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "lib": [
      "es5", "es6", "dom", "dom.iterable"
  ]
},
"exclude": [
    "node_modules",
    "**/node_modules/*"
]
}
